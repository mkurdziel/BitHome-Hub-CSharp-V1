<UserControl x:Class="SyNet.XamlPanels.DeviceConfigSet"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <StackPanel>
    <StackPanel Orientation="Horizontal">
      <Label>SyNet ID</Label>
      <TextBox x:Name="m_TextBoxID" Width="100"></TextBox>
      <Button Name="ButtonID" Click="ButtonID_Click">Set</Button>
    </StackPanel>
    <StackPanel Orientation="Horizontal">
      <Label>Manufacturer</Label>
      <TextBox x:Name="m_TextBoxManufac" Width="100"></TextBox>
      <Button Name="ButtonManufac" Click="ButtonManufac_Click">Set</Button>
    </StackPanel>
    <StackPanel Orientation="Horizontal">
      <Label>Profile</Label>
      <TextBox x:Name="m_TextBoxProfile" Width="100"></TextBox>
      <Button Name="ButtonProfile" Click="ButtonProfile_Click">Set</Button>
    </StackPanel>
    <StackPanel Orientation="Horizontal">
      <Label>Revision</Label>
      <TextBox x:Name="m_TextBoxRevision" Width="100"></TextBox>
      <Button Name="ButtonRevision" Click="ButtonRevision_Click">Set</Button>
    </StackPanel>
    <StackPanel Orientation="Horizontal">
      <Label>Remote</Label>
      <TextBox x:Name="m_TextBoxRemote" Width="100"></TextBox>
      <Button Name="ButtonRemote" Click="ButtonRemote_Click">Set</Button>
    </StackPanel>
  </StackPanel>
</UserControl>
